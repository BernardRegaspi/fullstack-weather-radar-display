{
  "name": "fullstack-weather-radar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node server/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "tsc -b && vite build",
    "start": "npm run build && npm run server",
    "start:prod": "concurrently \"npm run preview\" \"npm run server\"",
    "lint": "eslint .",
    "preview": "vite preview --port 5173 --host"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "grib2-simple": "^1.1.1",
    "leaflet": "^1.9.4",
    "pngjs": "^7.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-leaflet": "^5.0.0",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/leaflet": "^1.9.14",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
